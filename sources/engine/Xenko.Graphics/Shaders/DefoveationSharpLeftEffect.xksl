shader DefoveationSharpLeftEffect : DefoveationSharpEffect
{
    stage override float4 Shading()
    {
        float2 uv = Defoveate(streams.TexCoord);
        uv.x = uv.x / 2;
		return float4(CAS(uv), 1.0);
    }
};
